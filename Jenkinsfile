#!/usr/bin/env groovy

/*
 * Jenkinsfile for testing Sumo Logic plugin
 */
pipeline {
    agent any 
    stages {
        stage('Build') {
            steps {
                echo 'Building!' 
            }
        }
        stage('Test') {
            steps {
                echo 'Testing!' 
            }
        }
        stage('Package') {
            steps {
                echo 'Packaging!' 
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deploying!' 
            }
        }
        stage('Release') {
            steps {
                echo 'Releasing!' 
            }
        }
    }
}